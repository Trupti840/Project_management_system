
<div class="card">
  <div class="card-header">
    <h2 class="card-title d-flex justify-content-between">
      <span>Projects</span>
    <%= link_to "New project", new_project_path ,class:"btn btn-primary" %>
    </h2>
  </div>
  <div class="card-body">
    <% @projects.each do |p|%>
        <div class="card">
          <div class="card-body">
            <h5 class="card-title"><%= link_to p.name.capitalize , project_path(p) %></h5>
            <p><%= p.description.capitalize %></p>
          </div>
        </div>
      <% end %>
  </div>
</div>


